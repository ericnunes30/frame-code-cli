{
  "name": "frame-code-cli",
  "version": "0.0.1",
  "description": "CLI para interagir com agentes de IA usando o frame-agent-sdk",
  "main": "dist/index.js",
  "bin": {
    "frame-code": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericnunes/frame-code-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ericnunes/frame-code-cli/issues"
  },
  "homepage": "https://github.com/ericnunes/frame-code-cli#readme",
  "scripts": {
    "build": "npm run build:skill-registry && tsc && npm run copy:prompts",
    "copy:prompts": "cpy \"src/prompts/**/*\" dist/prompts --parents",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "interactive": "chmod +x src/scripts/capture-logs.sh && src/scripts/capture-logs.sh npm run dev interactive",
    "dev:logs": "bash src/scripts/capture-logs.sh npm run dev interactive",
    "dev:start": "npm run dev interactive",
    "log:view": "tail -f server.log",
    "log:clear": "rm -f server.log",
    "log:stats": "ls -la server.log",
    "clean": "rm -rf dist",
    "build:skill-registry": "ts-node --transpile-only src/scripts/buildSkillRegistry.ts"
  },
  "keywords": [
    "cli",
    "agent",
    "ai",
    "framework"
  ],
  "author": "Eric Nunes",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@typescript-eslint/typescript-estree": "^8.48.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "eventsource": "^4.1.0",
    "fast-diff": "^1.3.0",
    "frame-agent-sdk": "file:../frame-agent-sdk",
    "glob": "^11.0.3",
    "inquirer": "^12.9.6",
    "openai": "^6.10.0",
    "tree-kill": "^1.2.2",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/keypress": "^2.0.30",
    "@types/node": "^24.7.0",
    "cpy-cli": "^6.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "types": "./dist/index.d.ts"
}
