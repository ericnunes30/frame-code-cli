{
  "name": "frame-code-cli",
  "version": "0.0.1",
  "description": "CLI para interagir com agentes de IA usando o frame-agent-sdk",
  "main": "dist/index.js",
  "bin": {
    "frame-code": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericnunes/frame-code-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ericnunes/frame-code-cli/issues"
  },
  "homepage": "https://github.com/ericnunes/frame-code-cli#readme",
  "scripts": {
    "build": "tsc && npm run copy:prompts",
    "copy:prompts": "xcopy /E /I /Y src\\\\prompts dist\\\\prompts || cp -r src/prompts dist/prompts",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "log:view": "tail -f server.log",
    "log:clear": "rm -f server.log",
    "log:stats": "ls -la server.log",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "agent",
    "ai",
    "framework"
  ],
  "author": "Eric Nunes",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "frame-agent-sdk": "file:../frame-agent-sdk",
    "glob": "^11.0.3",
    "inquirer": "^12.9.6",
    "openai": "^6.2.0",
    "ora": "^9.0.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^24.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "types": "./dist/index.d.ts"
}