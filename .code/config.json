{
  "_comment": "Configurações de modelos por agente",
  "_comment_usage": "Copie este arquivo para .code/config.json e ajuste conforme necessário",
  "_comment_env": "Variáveis de ambiente usam sintaxe ${VAR} ou ${VAR:-default} (com valor padrão)",
  "_comment_priority": "Prioridade: frontmatter do agente > config.json agente > config.json defaults > ENV > hardcoded",
  "_comment_models": "Modelos disponíveis no provedor openai-compatible:",
  "_comment_models_list": [
    "Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8 - Modelo principal para código",
    "Qwen/Qwen2.5-72B-Instruct - Modelo geral",
    "meta-llama/Llama-3.1-70B-Instruct - Llama 70B",
    "deepseek-ai/DeepSeek-V3 - DeepSeek para raciocínio"
  ],
  "defaults": {
    "provider": "${LLM_PROVIDER}",
    "model": "${LLM_DEFAULT_MODEL",
    "apiKey": "${LLM_API_KEY}",
    "baseUrl": "${LLM_BASE_URL",
    "temperature": "${LLM_TEMPERATURE:-0.7}",
    "maxTokens": "${LLM_MAX_OUTPUT_TOKENS:-4096}",
    "maxContextTokens": "${LLM_MAX_TOKENS:-128000}",
    "topP": "1.0",
    "capabilities": {
      "supportsVision": "${LLM_SUPPORTS_VISION:-true}"
    }
  },
  "agents": {
    "code-agent": {
      "provider": "${LLM_PROVIDER}",
      "model": "${LLM_DEFAULT_MODEL}",
      "apiKey": "${LLM_API_KEY}",
      "baseUrl": "${LLM_BASE_URL}",
      "temperature": 0.3,
      "maxTokens": 8192,
      "maxContextTokens": 230000,
      "topP": 0.95,
      "capabilities": {
        "supportsVision": false
      }
    },
    "builder": {
      "provider": "${LLM_PROVIDER:-openai-compatible}",
      "model": "Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8",
      "apiKey": "${LLM_API_KEY}",
      "baseUrl": "${LLM_BASE_URL}",
      "temperature": 0.2,
      "maxTokens": 8192,
      "maxContextTokens": 128000,
      "topP": 0.9,
      "capabilities": {
        "supportsVision": false
      }
    },
    "architect": {
      "provider": "${LLM_PROVIDER:-openai-compatible}",
      "model": "zai-org/GLM-4.6-TEE",
      "apiKey": "${LLM_API_KEY}",
      "baseUrl": "${LLM_BASE_URL}",
      "temperature": 0.3,
      "maxTokens": 6144,
      "maxContextTokens": 128000,
      "topP": 0.9,
      "capabilities": {
        "supportsVision": false
      }
    },
    "supervisor": {
      "provider": "${LLM_PROVIDER:-openai-compatible}",
      "model": "zai-org/GLM-4.6-TEE",
      "apiKey": "${LLM_API_KEY}",
      "baseUrl": "${LLM_BASE_URL}",
      "temperature": 0.3,
      "maxTokens": 4096,
      "maxContextTokens": 128000,
      "topP": 0.9,
      "capabilities": {
        "supportsVision": false
      }
    },
    "chrome-mcp-agent": {
      "provider": "${LLM_PROVIDER:-openai-compatible}",
      "model": "Qwen/Qwen2.5-VL-72B-Instruct-TEE",
      "apiKey": "${LLM_API_KEY}",
      "baseUrl": "${LLM_BASE_URL}",
      "temperature": 0.3,
      "maxTokens": 4096,
      "maxContextTokens": 128000,
      "topP": 0.9,
      "capabilities": {
        "supportsVision": true
      }
    }
  }
}
